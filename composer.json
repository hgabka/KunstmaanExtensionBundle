{
    "name": "hgabka/kunstmaan-extension-bundle",
    "type": "kunstmaan-bundle",
    "description": "Kunstmaan extension bundle",
    "keywords": [
    ],
    "homepage": "https://github.com/hgabka/KunstmaanExtensionBundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gabe",
            "email": "hgabka@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "symfony/symfony": "~2.7||~3",
        "kunstmaan/bundles-cms": "dev-master",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "fchris82/jms-translation-bundle-fork": "~1.2.2",
        "prezent/doctrine-translatable-bundle": "^1.0",
        "a2lix/translation-form-bundle": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Hgabka\\KunstmaanExtensionBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hgabka\\KunstmaanExtensionBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
